>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>Javascript File>>>>>>>>>>>>>>>>>>>>>>>>>

function login(){
    document.querySelector(".popup").style.display="flex";
    document.querySelector(".carousel-caption").style.display="none";
}


  function hidePop(){
  
    document.querySelector(".popup").style.display="none";
  
  }


function validate(){
    if ("pas1"!="pas2"){
        alert("Passwords are not matching");
    }
    else{
       return true;
    }
}



